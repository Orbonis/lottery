{
    "compilerOptions": {
        "target": "es2016",
        "jsx": "react",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "baseUrl": ".",
        "paths": {
            "src": [
                "src"
            ],
            "core": [
                "libs/*/src"
            ]
        }
    }
}